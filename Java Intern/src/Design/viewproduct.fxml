                    </Button>
                    <TextField fx:id="search" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="200.0" promptText="Search" GridPane.halignment="RIGHT" GridPane.valignment="BOTTOM">
                        <GridPane.margin>
                            <Insets right="100.0" />
                        </GridPane.margin>
                    </TextField>
                </children>
            </GridPane>
         </children>
      </HBox>
   </children>
</VBox>
