<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Viewproduct">
   <children>
      <HBox alignment="CENTER">
         <children>
            <GridPane alignment="CENTER" prefHeight="650.0" prefWidth="682.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints maxHeight="40.0" minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="610.0" minHeight="10.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <TableView fx:id="tbl" prefHeight="610.0" prefWidth="677.0" GridPane.rowIndex="1">
                        <columns>
                            <TableColumn fx:id="id1" prefWidth="75.0" text="Sr.No." />
                            <TableColumn fx:id="prodname" prefWidth="149.0" text="Product Name" />
                            <TableColumn fx:id="dom" prefWidth="153.0" text="DOM" />
                            <TableColumn fx:id="hsn" minWidth="0.0" prefWidth="154.0" text="HSN" />
                            <TableColumn fx:id="gst" prefWidth="157.0" text="GST" />
                        </columns>
                    </TableView>
                    <Button fx:id="export" mnemonicParsing="false" prefHeight="31.0" prefWidth="72.0" text="Export" GridPane.halignment="RIGHT" GridPane.valignment="BOTTOM">
